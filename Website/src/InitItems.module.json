{
  "namespace": "InitItems",
  "items": {
    "includes": [
      {
        "name": "content-root",
        "path": "/sitecore/content",
        "scope": "singleItem",
        "allowedPushOperations": "createAndUpdate"
      },
      {
        "name": "api-key",
        "path": "/sitecore/system/Settings/Services/API Keys/EdgeWebsite",
        "scope": "singleItem"
      },
      {
        "name": "core-fields",
        "path": "/sitecore/system/Field types/EdgeWebsite Types",
        "database": "core"
      },
      {
        "name": "languages",
        "path": "/sitecore/system/Languages",
        "scope": "descendantsOnly",
        "rules": [
          {
            "path": "/en",
            "scope": "ignored"
          }
        ]
      }
    ]
  }
}
