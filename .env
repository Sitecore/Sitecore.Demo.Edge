AUTH0_SECRET=6603f3bcc0a9b9f9e564b6cd16c1037c122fdb2bf0f4f90c5432b5296b4a8a46
AUTH0_BASE_URL=https://www.edge.localhost/shop
AUTH0_ISSUER_BASE_URL=https://playdemo.us.auth0.com
